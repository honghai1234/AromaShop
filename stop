[32m+[m[32m(function webpackUniversalModuleDefinition(root, factory) {[m
[32m+[m	[32mif(typeof exports === 'object' && typeof module === 'object')[m
[32m+[m		[32mmodule.exports = factory();[m
[32m+[m	[32melse if(typeof define === 'function' && define.amd)[m
[32m+[m		[32mdefine([], factory);[m
[32m+[m	[32melse if(typeof exports === 'object')[m
[32m+[m		[32mexports["Choices"] = factory();[m
[32m+[m	[32melse[m
[32m+[m		[32mroot["Choices"] = factory();[m
[32m+[m[32m})(this, function() {[m
[32m+[m[32mreturn /******/ (function(modules) { // webpackBootstrap[m
[32m+[m[32m/******/ 	// The module cache[m
[32m+[m[32m/******/ 	var installedModules = {};[m
[32m+[m
[32m+[m[32m/******/ 	// The require function[m
[32m+[m[32m/******/ 	function __webpack_require__(moduleId) {[m
[32m+[m
[32m+[m[32m/******/ 		// Check if module is in cache[m
[32m+[m[32m/******/ 		if(installedModules[moduleId])[m
[32m+[m[32m/******/ 			return installedModules[moduleId].exports;[m
[32m+[m
[32m+[m[32m/******/ 		// Create a new module (and put it into the cache)[m
[32m+[m[32m/******/ 		var module = installedModules[moduleId] = {[m
[32m+[m[32m/******/ 			exports: {},[m
[32m+[m[32m/******/ 			id: moduleId,[m
[32m+[m[32m/******/ 			loaded: false[m
[32m+[m[32m/******/ 		};[m
[32m+[m
[32m+[m[32m/******/ 		// Execute the module function[m
[32m+[m[32m/******/ 		modules[moduleId].call(module.exports, module, module.exports, __webpack_require__);[m
