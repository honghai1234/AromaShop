[32m+[m[32mif (!function_exists('formatDate')) {[m
[32m+[m[32m    function formatDate($date, string $format = 'Y/m/d')[m
[32m+[m[32m    {[m
[32m+[m[32m        if ($date instanceof \Carbon\Carbon) {[m
[32m+[m[32m            return $date->format($format);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $date;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m[32mif (!function_exists('arrayColor')) {[m
[32m+[m[32m    function arrayColor($color)[m
[32m+[m[32m    {[m
[32m+[m[32m        $array = ["", "Red", "Blue", "Yellow"];[m
[32m+[m[32m        return $array[$color];[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
